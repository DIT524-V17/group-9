<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="team9_dit524.mobileapplication.Control"
    android:background="@drawable/googra"
    tools:showIn="@layout/activity_control">

    <Button
        android:id="@+id/forward"
        android:layout_width="105dp"
        android:layout_height="61dp"
        android:background="@drawable/arrow"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="127dp" />

    <Button
        android:id="@+id/left"
        android:layout_width="61dp"
        android:layout_height="95dp"
        android:background="@drawable/arrowleft"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="208dp" />

    <Button
        android:id="@+id/right"
        android:layout_width="61dp"
        android:layout_height="95dp"
        android:background="@drawable/arrowright"
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="208dp" />

    <Button
        android:id="@+id/backward"
        android:layout_width="105dp"
        android:layout_height="62dp"
        android:background="@drawable/arrowback"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="321dp" />

    <Button
        android:id="@+id/stop"
        android:layout_width="92dp"
        android:layout_height="78dp"
        android:background="@drawable/stop"
        tools:layout_editor_absoluteX="276dp"
        tools:layout_editor_absoluteY="417dp" />

    <Button
        android:id="@+id/Alert"
        android:layout_width="86dp"
        android:layout_height="75dp"
        android:background="@drawable/alert"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="420dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="380dp"
        android:layout_height="183dp"
        app:srcCompat="@drawable/menace"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="-32dp" />

</android.support.constraint.ConstraintLayout>
